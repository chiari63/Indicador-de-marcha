void setup() {
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
  
  // Animação de inicialização com logo
  displayFrame(epd_bitmap_3);
  delay(10);
  displayFrame(epd_bitmap_4);
  delay(10);
  displayFrame(epd_bitmap_5);
  delay(10);
  displayFrame(epd_bitmap_6);
  delay(10);
  displayFrame(epd_bitmap_7);
  delay(10);
  displayFrame(epd_bitmap_8);
  delay(10);
  displayFrame(epd_bitmap_9);
  delay(10);
  displayFrame(epd_bitmap_10);
  delay(10);
  displayFrame(epd_bitmap_11);
  delay(10);
  displayFrame(epd_bitmap_12);
  delay(10);
  displayFrame(epd_bitmap_13);
  delay(10);
  displayFrame(epd_bitmap_14);
  delay(10);
  displayFrame(epd_bitmap_15);
  delay(10);
  displayFrame(epd_bitmap_16);
  delay(10);
  displayFrame(epd_bitmap_17);
  delay(10);
  displayFrame(epd_bitmap_18);
  delay(1000);

  // Configura os pinos dos botões como entradas
  for (int i = 0; i < 6; i++) {
    pinMode(buttonPins[i], INPUT_PULLUP);
  }

  // Exibe "N" inicial
  displayGear();
}

void loop() {
  for (int i = 0; i < 6; i++) {
    if (digitalRead(buttonPins[i]) == LOW) {
      currentGear = gears[i];
      displayGear();
      while (digitalRead(buttonPins[i]) == LOW) {}
      currentGear = 'N';
      displayGear();
    }
  }
}

void displayGear() {
  display.clearDisplay();
  display.setTextSize(7);
  display.setTextColor(SSD1306_WHITE);
  display.setCursor(50, 10);
  display.print(currentGear);
  display.display();
}

void displayFrame(const unsigned char* frame) {
  display.clearDisplay();
  display.drawBitmap(0, 0, frame, SCREEN_WIDTH, SCREEN_HEIGHT, SSD1306_WHITE);
  display.display();
}
